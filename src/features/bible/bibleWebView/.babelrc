{
  "presets": [
      "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/transform-react-jsx"]
  ],
  "env": {
    "production": {
      "plugins": ["emotion"]
    },
    "development": {
      "plugins": [["emotion", { "sourceMap": true }]]
    }
  }
}