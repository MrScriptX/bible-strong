{
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}/src",
    "src": "${workspaceRoot}/src",
    "~common": "${workspaceRoot}/src/common",
    "~features": "${workspaceRoot}/src/features",
    "~helpers": "${workspaceRoot}/src/helpers",
    "~navigation": "${workspaceRoot}/src/navigation",
    "~redux": "${workspaceRoot}/src/redux",
    "~themes": "${workspaceRoot}/src/themes"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
