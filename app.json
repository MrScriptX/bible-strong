{
  "expo": {
    "name": "bible-strong",
    "description": "Bible strong for french people",
    "slug": "bible-strong",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "primaryColor": "#0ED3B9",
    "githubUrl": "https://github.com/bulby97/bible-strong",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.2.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "assetExts": [
        "sqlite",
        "txt"
      ]
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "src/assets/bible_versions/*",
      "src/assets/database/*",
      "src/assets/images/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.smontlouis.biblestrong",
      "buildNumber": "7"
    },
    "android": {
      "package": "com.smontlouis.biblestrong",
      "versionCode": 7
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "smontlouis",
            "project": "bible-strong",
            "authToken": "87f5b33659ef43e498fb00d8cc80fdc14f971e1c45c140bab4283c056dbac749"
          }
        }
      ]
    }
  }
}